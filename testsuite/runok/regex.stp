#! stap -p5

global n
global pass, fail

@define check (code, regexp, str) %(
  result = (@str =~ @regexp);
  n++;
  if (result == !@code) {
    printf("PASS: #%d: %s %s %s\n", n, @regexp, (@code ? "!~" : "=~"), @str);
    pass++
  } else {
    printf("FAIL: #%d: %s %s %s\n", n, @regexp, (@code ? "!~" : "=~"), @str);
    fail++
  }
%)

